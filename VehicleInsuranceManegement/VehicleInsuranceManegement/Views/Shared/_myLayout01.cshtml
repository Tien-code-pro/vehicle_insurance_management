<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/assets/css/style.css")
    <link href="~/assets/css/hover-min.css" rel="stylesheet" />
    <link href="~/assets/css/animate.css" rel="stylesheet" />
    <link href="~/assets/css/bootsnav.css" rel="stylesheet" />
    <link href="~/assets/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/assets/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/assets/css/magnific-popup.css" rel="stylesheet" />
    <link href="~/assets/css/owl.carousel.min.css" rel="stylesheet" />
    <link href="~/assets/css/owl.theme.default.min.css" rel="stylesheet" />
    <link href="~/assets/css/responsive.css" rel="stylesheet" />
    <link href="~/assets/css/style.css" rel="stylesheet" />

    <body>
        <!--start header-->
        <div class="header">
        </div>
        <!--end header-->
        @{ Html.RenderPartial("_header");}
        <!--start body-->
        <div>
            @RenderBody()
        </div>
        <!--end body-->
        <!--start footer-->
        <div class="footer">
            @{Html.RenderPartial("_footer");}
        </div>
        <!--end footer-->
    </body>
    <script src="~/assets/js/register-and-value.js"></script>
    <script src="~/assets/js/waypoints.min.js"></script>
    <script src="~/assets/js/owl.carousel.min.js"></script>
    <script src="~/assets/js/online-quotation.js"></script>
    <script src="~/assets/js/name-and-contact.js"></script>
    <script src="~/assets/js/jquery.magnific-popup.min.js"></script>
    <script src="~/assets/js/jquery.hc-sticky.min.js"></script>
    <script src="~/assets/js/jquery.counterup.min.js"></script>
    <script src="~/assets/js/jak-menusearch.js"></script>
    <script src="~/assets/js/custom.js"></script>
    <script src="~/assets/js/car-of-information.js"></script>
    <script src="~/assets/js/bootstrap.min.js"></script>
    <script src="~/assets/js/bootsnav.js"></script>
    <script src="~/assets/js/about.js"></script>
    <script src="~/assets/js/jquery.js"></script>
</html>
